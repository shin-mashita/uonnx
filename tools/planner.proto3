syntax = "proto3";

package planner;

message Plan {
    uint32 id = 1;
    int32 start_idx = 2;
    int32 type = 3;
    repeated int32 dims = 4;
}

message Planner {

    message ArenaProfile {
        int32 max_ntensors = 1;
        int32 max_bytes = 2;
    }

    ArenaProfile arena = 1;
    repeated Plan plans = 2;
}